{
    "workbench.startupEditor": "newUntitledFile",
    "workbench.colorTheme": "Atom One Dark",
    "workbench.statusBar.visible": true,
    "workbench.iconTheme": "file-icons",
    "files.autoSave": "onWindowChange",
    "editor.fontFamily": "'InconsolataGo Nerd Font', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 120,
    "window.menuBarVisibility": "hidden",
    "vim.cursorStylePerMode.insert": "line",
    "vim.cursorStylePerMode.normal": "block",
    "vim.cursorStylePerMode.replace": "underline",
    "vim.cursorStylePerMode.visual": "block",
    "vim.cursorStylePerMode.visualblock": "block",
    "vim.cursorStylePerMode.visualline": "block",
    "vim.debug.loggingLevelForAlert": "verbose",
    "vim.hlsearch": true,
    "vim.leader": "",
    "vim.vimrc.enable": true,
    "vim.vimrc.path": "$HOME/.vimrc",
    "vim.visualstar": true,
    "editor.minimap.enabled": false,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false
}